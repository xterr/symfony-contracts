{
    "name": "xterr/symfony-contracts",
    "type": "library",
    "description": "A set of abstractions extracted out of the Symfony components",
    "keywords": ["abstractions", "contracts", "decoupling", "interfaces", "interoperability", "standards"],
    "license": "MIT",
    "authors": [
        {
            "name": "Razvan Ceana",
            "email": "razvan@ceana.ro"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "suggest": {
        "doctrine/orm": "When using Doctrine contracts",
        "gedmo/doctrine-extensions": "When using Gedmo extensions contracts"
    },
    "autoload": {
        "psr-4": { "Xterr\\Symfony\\Contracts\\": "" },
        "exclude-from-classmap": [
            "**/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
